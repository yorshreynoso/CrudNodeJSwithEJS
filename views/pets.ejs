<%-include('template/header', {titleWeb :"Index"}) %> 

   <div class="container">
        <table class="table">
            <thead>
                <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Description</th>
                <th scope="col">Action</th>
                </tr>
            </thead>
            <tbody>
                <% if (arrayPet.length > 0) { %>
                    <% arrayPet.forEach(pet => { %>
                        <tr>
                            <th scope="row"><%= pet.id  %> </th>
                            <td> <%=pet.name%></td>
                            <td> <%=pet.description %></td>
                            <td>algo aqui</td>
                        </tr>
                    <% }) %>
                <% } %>
            </tbody>
        </table>
   </div>

<%-include('template/footer') %> 
